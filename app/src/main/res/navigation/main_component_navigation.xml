<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_component_navigation"
    app:startDestination="@id/feedFragment">

    <fragment
        android:id="@+id/feedFragment"
        android:name="com.nayeem2021.liilab_app_dev_assesment_project.FeedFragment"
        android:label="fragment_feed"
        tools:layout="@layout/fragment_feed" >
        <action
            android:id="@+id/action_feedFragment_to_createPostFragment"
            app:destination="@id/createPostFragment" />
    </fragment>
    <fragment
        android:id="@+id/myCommunityFragment2"
        android:name="com.nayeem2021.liilab_app_dev_assesment_project.MyCommunityFragment"
        android:label="fragment_my_community"
        tools:layout="@layout/fragment_my_community" />
    <fragment
        android:id="@+id/exploreFragment"
        android:name="com.nayeem2021.liilab_app_dev_assesment_project.presentation.explore.ExploreFragment"
        android:label="fragment_explore"
        tools:layout="@layout/fragment_explore" />
    <fragment
        android:id="@+id/notificationFragment"
        android:name="com.nayeem2021.liilab_app_dev_assesment_project.presentation.notification.NotificationFragment"
        android:label="fragment_notification"
        tools:layout="@layout/fragment_notification" />
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.nayeem2021.liilab_app_dev_assesment_project.presentation.settings.SettingsFragment"
        android:label="SettingsFragment" />
    <fragment
        android:id="@+id/createPostFragment"
        android:name="com.nayeem2021.liilab_app_dev_assesment_project.CreatePostFragment"
        android:label="CreatePostFragment" />
</navigation>